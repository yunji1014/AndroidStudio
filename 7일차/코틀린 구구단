fun main() {
    //5단에서 8단까지 출력
    for(i in 5..8 step 1)
        for(j in 1..9 step 1)
            println("$i X $j = ${i*j} \n")

    //단 사이에 줄(************) 넣기
    for(i in 5..8 step 1){
        for(j in 1..9 step 1)
            println("$i X $j = ${i*j} \n")
        println("************")
    }
}
